// ==================\nimport css from \"./index.scss\";\n\n// ==================\n// 组件\n// ==================\n@connect(\n  state => ({}),\n  dispatch => ({\n    actions: bindActionCreators({}, dispatch)\n  })\n)\nexport default class FeaturesPageContainer extends React.Component {\n  static propTypes = {\n    location: P.any,\n    history: P.any,\n    actions: P.any\n  };\n\n  constructor(props) {\n    super(props);\n    this.state = {};\n  }\n\n  render() {\n    return (\n      <div className={css.page}>\n        <h1 className={css.title}>构建与特性</h1>\n        <div className={css.box}>\n          <div className={css.list}>\n            <h2>安装依赖文件</h2>\n            <p>npm install</p>\n          </div>\n          <div className={css.list}>\n            <h2>启动开发环境</h2>\n            <p>npm run dev</p>\n            <div>代码打包编译，默认监听8888端口</div>\n            <div>访问http://localhost:8888 即可查看</div>\n          </div>\n          <div className={css.list}>\n            <h2>正式打包</h2>\n            <p>npm run build</p>\n            <div>会将最终代码打包至/build文件夹中</div>\n            <div>\n              /build文件夹中的index.html 和 /build/dist文件夹\n              仅这两样东西是最终需要的\n            </div>\n          </div>\n          <div className={css.list}>\n            <h2>运行生产环境的代码</h2>\n            <p>npm run dist</p>\n            <div>运行build文件夹下生成好的最终代码</div>\n          </div>\n          <div className={css.list}>\n            <h2>HMR局部热更新</h2>\n            <div>\n              使用webpack-dev-middleware 和 webpack-hot-middleware设置了热更新\n            </div>\n          </div>\n          <div className={css.list}>\n            <h2>代码分割</h2>\n            <div>bundle-loader实现的代码分割</div>\n            <div>src/a_container/root/index.js中能查看例子</div>\n          </div>\n          <div className={css.list}>\n            <h2>HappyPack多线程编译</h2>\n            <div>使用了HappyPack插件，使编译速度更快</div>\n          </div>\n        </div>\n      </div>\n    );\n  }\n}\n"},"lineCount":null}},"hash":"7caf97ee0659ced63938144cd2e1d371","cacheData":{"env":{}}}